  "dependencies": {
    "@cursor-rules-todoapp/domain": "workspace:*",
    "@cursor-rules-todoapp/repo-sqlite": "workspace:*",
    "@prisma/client": "^5.22.0",
    "@trpc/server": "^10.45.0",
    "@types/node": "^22.10.10",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cursor-rules-todoapp/configs": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}